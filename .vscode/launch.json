{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch KidsPrize.Http",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceRoot}/src/KidsPrize.Http/bin/Debug/netcoreapp1.0/KidsPrize.Http.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/KidsPrize.Http/",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": false,
        "args": "${auto-detect-url}/swagger/ui/",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}/swagger/ui/"
        },
        "osx": {
          "command": "open"
        },
        "linux": {
          "command": "xdg-open"
        }
      },
      "env": {
        "ASPNETCORE_URLS": "http://localhost:5000",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Launch IdentityServer",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceRoot}/src/IdentityServer/bin/Debug/netcoreapp1.0/IdentityServer.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/IdentityServer/",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": false,
        "args": "${auto-detect-url}",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}"
        },
        "osx": {
          "command": "open"
        },
        "linux": {
          "command": "xdg-open"
        }
      },
      "env": {
        "ASPNETCORE_URLS": "http://localhost:4999",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": 0
    }
  ]
}